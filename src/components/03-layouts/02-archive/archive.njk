<div class="page-header">
	{% render '@breadcrumb-link', {label: 'Home', href: '/home'} %}
	<h1>{{ title }}</h1>
</div>
<div class="filter-sort">
	{% render '@button', {standAlone: true, id: 'invoke-dialog'}, true %}
	<div class="filter-sort__filters">
		<h2 class="screen-reader-text">Filters</h2>
		{% render '@accordion', {accordions: accordions, standAlone: true}, true %}
	</div>
	<div class="filter-sort__sorts">
	</div>
</div>
<ul class="cards">
{% for card in cards %}
  {% render '@card--resource', card %}
{% endfor %}
</ul>
{% render '@pagination', {title: false}, true %}
